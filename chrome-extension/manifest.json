{
	"name": "X-FBKINGDOM",
	"version": "1.0.0",
	"manifest_version": 3,
	"permissions": [
	],
	"description": "X(旧Twitter)のフォントを FBKINGDOM-FONT に置き換えます",
	"content_scripts": [
		{
			"matches": [
				"https://x.com/*"
			],
			"css": ["style.css"]
		}
	],
	"web_accessible_resources": [
		{
			"resources": [
				"*.otf",
				"*.css"
			],
			"matches": [
				"https://x.com/*"
			]
		}
	]
}
